{
  "compilerOptions": {
    "noImplicitAny": true,
    "skipLibCheck": true,
    "pretty": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "jsx": "react",
    "target": "es5",
    "module": "commonjs",
    "typeRoots": [
      "node_modules/@types/",
      "types/"
    ]
  }
}
